<% @cats.each do |cat| %>
  <%= cat.name %>
<% end %>

<% @todos.each do |todo| %>
  <%= todo.tasks %>
  <%= todo.finished %>
<% end %>

<% @users.each do |user| %>
  <%= user.username %>
  <%= user.email %>
  <%= user.age %>
<% end %>

<%= form_for @todo do |x| %>
  <%= x.label :tasks %>
  <%= x.text_field :tasks %>
  <%= x.label :finished %>
  <%= x.text_field :finished %>
<%= submit_tag("Create") %>
<% end %>

